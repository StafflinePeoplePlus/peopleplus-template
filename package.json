{
	"name": "peopleplus-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "wrangler pages dev --port 4173 --compatibility-date=2023-05-18 .svelte-kit/cloudflare/",
		"test": "playwright test",
		"test-coverage": "VITE_COVERAGE=true nyc --instrument=false playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit-coverage": "vitest run --coverage",
		"lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint . && npm run check",
		"format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"merge-coverage": "istanbul report -v --include='coverage/**/coverage-final.json' json-summary"
	},
	"devDependencies": {
		"@actions/github": "^6.0.0",
		"@auth/core": "^0.23.0",
		"@auth/sveltekit": "^0.9.0",
		"@cloudflare/workers-types": "^4.20240117.0",
		"@playwright/test": "^1.41.1",
		"@sveltejs/adapter-cloudflare": "^2.3.4",
		"@sveltejs/kit": "^1.27.7",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"@vitest/coverage-istanbul": "^1.2.2",
		"autoprefixer": "^10.4.17",
		"dotenv": "^16.4.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"istanbul": "^0.4.5",
		"istanbul-badges-readme": "^1.8.5",
		"jsdom": "^24.0.0",
		"kayle": "^0.8.1",
		"nyc": "^15.1.0",
		"playwright-test-coverage": "^1.2.12",
		"postcss": "^8.4.33",
		"pp-svelte-components": "^0.0.36",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^4.5.0",
		"vite-plugin-istanbul": "^5.0.0",
		"vitest": "^1.2.2",
		"wrangler": "^3.25.0",
		"zod": "^3.22.4"
	},
	"type": "module"
}
