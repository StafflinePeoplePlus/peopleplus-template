{
	"name": "peopleplus-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "wrangler pages dev --port 4173 --compatibility-date=2023-05-18 .svelte-kit/cloudflare/",
		"test": "playwright test",
		"test-coverage": "VITE_COVERAGE=true nyc --instrument=false playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit-coverage": "vitest run --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"merge-coverage": "istanbul report -v --include='coverage/**/coverage-final.json' json-summary"
	},
	"devDependencies": {
		"@auth/core": "^0.7.1",
		"@auth/sveltekit": "^0.3.1",
		"@cloudflare/workers-types": "^4.20230518.0",
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-cloudflare": "^2.3.0",
		"@sveltejs/kit": "^1.20.0",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"@vitest/coverage-istanbul": "^0.31.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"istanbul": "^0.4.5",
		"istanbul-badges-readme": "^1.8.5",
		"jsdom": "^22.0.0",
		"nyc": "^15.1.0",
		"playwright-test-coverage": "^1.2.12",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-istanbul": "^4.1.0",
		"vitest": "^0.32.0",
		"wrangler": "^3.0.1"
	},
	"type": "module"
}
