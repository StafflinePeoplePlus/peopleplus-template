{
	"name": "peopleplus-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "wrangler pages dev --port 4173 --compatibility-date=2023-05-18 .svelte-kit/cloudflare/",
		"test": "playwright test",
		"test-coverage": "VITE_COVERAGE=true nyc --instrument=false playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit-coverage": "vitest run --coverage",
		"lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint . && npm run check",
		"format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"merge-coverage": "istanbul report -v --include='coverage/**/coverage-final.json' json-summary"
	},
	"devDependencies": {
		"@actions/github": "^6.0.0",
		"@auth/core": "^0.17.0",
		"@auth/sveltekit": "^0.3.10",
		"@cloudflare/workers-types": "^4.20231016.0",
		"@playwright/test": "^1.39.0",
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"@sveltejs/kit": "^1.27.0",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"@vitest/coverage-istanbul": "^0.34.1",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.3.1",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.0",
		"istanbul": "^0.4.5",
		"istanbul-badges-readme": "^1.8.5",
		"jsdom": "^22.1.0",
		"kayle": "^0.7.12",
		"nyc": "^15.1.0",
		"playwright-test-coverage": "^1.2.12",
		"postcss": "^8.4.30",
		"pp-svelte-components": "^0.0.29",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"svelte": "^4.2.2",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.4",
		"tslib": "^2.6.2",
		"typescript": "^5.1.6",
		"vite": "^4.5.0",
		"vite-plugin-istanbul": "^5.0.0",
		"vitest": "^0.34.6",
		"wrangler": "^3.14.0",
		"zod": "^3.22.4"
	},
	"type": "module"
}
